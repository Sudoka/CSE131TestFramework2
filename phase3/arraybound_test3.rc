structdef FOO {
	int a;
	float b;
	float[1000] c;
};

function : void main()
{
	int b = 1;
	int a = 500 + 500 - 1 + b - b;
	
	FOO gandalf;
	
	gandalf.c[b] = 9;
	
	cout << gandalf.c[b] << endl;
	
	gandalf.c[a] = 100;
	
	cout << gandalf.c[a] << endl;
	
	gandalf.c[a+1-1+b-b+b-b] = 200;
	
	cout << gandalf.c[a+1-1+b-b+b-b] << endl;
}